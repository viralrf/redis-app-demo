
<h1>Agregar usuario</h1>
{{#if error}} <span>{{error}}</span>{{/if}}
<form  method="POST" action="/agregar">
	<div class="form-group">
		<label>ID</label>
		<input type="text" name="id" class="form-control" placeholder="Agregar ID">
	</div>
	<div class="form-group">
		<label>Nombre</label>
		<input type="text" name="nombre" class="form-control" placeholder="Agregar nombre">
	</div>
	<div class="form-group">
		<label>Apellido</label>
		<input type="text" name="apellido" class="form-control" placeholder="Agregar apellido">
	</div>
	<div class="form-group">
		<label>Email</label>
		<input type="text" name="email" class="form-control" placeholder="Agregar Email">
	</div>
	<div class="form-group">
		<label>Fono</label>
		<input type="text" name="fono" class="form-control" placeholder="Agregar fono">
	</div>
	
	<input type="submit" class="btn btn-primary" value="Guardar">
</form>